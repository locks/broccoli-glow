    "test:debug": "npm run build && mocha debug dist/tests",
    "test:watch": "testem",
    "prepublish": "npm run build"
  },
  "keywords": [
    "broccoli",
    "plugin"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
  },
  "homepage": "",
  "files": [
    "dist/",
    "!dist/tests"
  ],
  "devDependencies": {
    "broccoli": "^1.1.1",
    "broccoli-babel-transpiler": "^5.6.0",
    "broccoli-cli": "^1.0.0",
    "broccoli-lint-eslint": "^3.2.0",
    "broccoli-merge-trees": "^2.0.0",
    "broccoli-stew": "^1.3.2",
    "broccoli-test-helper": "^1.0.0",
    "chai": "^3.5.0",
    "mocha": "^3.2.0",
    "mocha-eslint": "^3.0.1",
    "regenerator-runtime": "^0.10.3",
    "testem": "^1.13.0"
  },
  "dependencies": {
    "broccoli-plugin": "^1.2.1",
